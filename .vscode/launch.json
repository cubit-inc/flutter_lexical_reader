    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_lexical_reader",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "flutter_lexical_reader (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutter_lexical_reader (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "Run flutter_lexical_reader tests",
            "type": "dart",
            "request": "launch",
            "program": "test/flutter_lexical_reader_test.dart", 
            "args": ["--no-pub"],
            "cwd": "${workspaceFolder}"
        }
    ]
}